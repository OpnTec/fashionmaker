#endif
#ifndef VOS__WINDOWS32
#define VOS__WINDOWS32          0x00000004L
#endif
#ifndef VOS_NT_WINDOWS32
#define VOS_NT_WINDOWS32        0x00040004L
#endif
// FILETYPE
